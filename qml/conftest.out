make: Nothing to be done for `all'.

testing c_compile:
echo "$conftest_c" >conftest.c
gcc -Wimplicit -Werror -c -o conftest.o conftest.c \
	    -m32 -march=i686     

testing c_link:
echo "$conftest_c" >conftest.c
gcc -Wimplicit -Werror -o conftest.exe conftest.c \
	    -m32 -march=i686          -lm

testing c_link with CFLAGS='-O2 -fno-strict-aliasing':
echo "$conftest_c" >conftest.c
gcc -Wimplicit -Werror -o conftest.exe conftest.c \
	    -m32 -march=i686     -O2 -fno-strict-aliasing      -lm

testing c_link with CFLAGS_FLOAT='-ffloat-store':
echo "$conftest_c" >conftest.c
gcc -Wimplicit -Werror -o conftest.exe conftest.c \
	    -m32 -march=i686     -O2 -fno-strict-aliasing -ffloat-store     -lm

testing xc_compile:
echo "$conftest_c" >conftest.c
gcc -c -o conftest.o conftest.c

testing xc_link:
echo "$conftest_c" >conftest.c
gcc -o conftest.exe conftest.c

testing xc_run:
echo "$conftest_c" >conftest.c
gcc -o conftest.exe conftest.c
./conftest.exe

testing f_compile:
echo "$conftest_f" >conftest.f
gfortran -Werror -c -o conftest.o conftest.f \
	    -m32 -march=i686     

testing f_link:
echo "$conftest_f" >conftest.f
gfortran -Werror -o conftest.exe conftest.f \
	    -m32 -march=i686          -lm
